{
  "name": "@officesdk/design-icons",
  "version": "0.1.0",
  "private": true,
  "description": "Built-in icon resources for OfficeSdk UI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate:registry": "tsx scripts/generate-registry.ts",
    "prebuild": "yarn generate:registry",
    "build": "yarn clean && tsup && yarn build:dts",
    "build:dts": "tsc --emitDeclarationOnly --outDir dist/esm && cp dist/esm/index.d.ts dist/cjs/index.d.ts",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "sideEffects": false
}
