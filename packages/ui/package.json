{
  "name": "@officesdk/ui",
  "version": "0.2.0",
  "description": "OfficeSdk SDK UI Component Library",
  "license": "MIT",
  "author": "OfficeSdk Team",
  "homepage": "https://github.com/officesdk/ui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/officesdk/ui.git"
  },
  "bugs": {
    "url": "https://github.com/officesdk/ui/issues"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "officesdk",
    "design-system",
    "styled-components"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "import": "./dist/icons/index.mjs",
      "require": "./dist/icons/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "styled-components": "^5.3.0"
  },
  "dependencies": {
    "@officesdk/ui-components": "workspace:*",
    "@officesdk/ui-icons": "workspace:*",
    "@officesdk/ui-theme": "workspace:*",
    "@officesdk/ui-utils": "workspace:*"
  },
  "scripts": {
    "build": "node build.js",
    "clean": "rm -rf dist"
  }
}
