{
  "name": "@officesdk/design-components",
  "version": "0.1.4",
  "description": "Officesdk  Design Components",
  "license": "MIT",
  "author": "OfficeSdk Team",
  "homepage": "https://github.com/officesdk/officesdk-design#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/officesdk/officesdk-design.git",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/officesdk/officesdk-design/issues"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "officesdk",
    "design-system"
  ],
  "private": true,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && tsup && yarn rename-mts",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "rename-mts": "mv dist/esm/index.d.mts dist/esm/index.d.ts"
  },
  "peerDependencies": {
    "rc-dropdown": "^4.0.0",
    "rc-menu": "^9.0.0",
    "rc-tooltip": "^6.0.0",
    "rc-virtual-list": "^3.0.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "styled-components": ">=5.2.0"
  },
  "dependencies": {
    "@officesdk/design-theme": "workspace:*",
    "@officesdk/design-utils": "workspace:*",
    "rc-dialog": "^10.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
