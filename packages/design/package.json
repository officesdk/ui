{
  "name": "@officesdk/design",
  "version": "0.1.8",
  "description": "OfficeSdk SDK Design Component Library",
  "license": "MIT",
  "author": "OfficeSdk Team",
  "homepage": "https://github.com/officesdk/officesdk-design#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/officesdk/officesdk-design.git"
  },
  "bugs": {
    "url": "https://github.com/officesdk/officesdk-design/issues"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "officesdk",
    "design-system",
    "styled-components"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/components/cjs/index.js",
  "module": "./dist/components/esm/index.js",
  "types": "./dist/components/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/components/esm/index.d.ts",
      "import": "./dist/components/esm/index.js",
      "require": "./dist/components/cjs/index.js"
    },
    "./theme": {
      "types": "./dist/theme/esm/index.d.ts",
      "import": "./dist/theme/esm/index.js",
      "require": "./dist/theme/cjs/index.js"
    },
    "./utils": {
      "types": "./dist/utils/esm/index.d.ts",
      "import": "./dist/utils/esm/index.js",
      "require": "./dist/utils/cjs/index.js"
    },
    "./icons": {
      "types": "./dist/icons/esm/index.d.ts",
      "import": "./dist/icons/esm/index.js",
      "require": "./dist/icons/cjs/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/components/esm/index.d.ts"
      ],
      "components": [
        "./dist/components/esm/index.d.ts"
      ],
      "theme": [
        "./dist/theme/esm/index.d.ts"
      ],
      "utils": [
        "./dist/utils/esm/index.d.ts"
      ],
      "icons": [
        "./dist/icons/esm/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "@officesdk/editor-sdk-core": "0.0.0-alpha.7",
    "rc-tooltip": "^6.0.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "styled-components": ">=5.2.0"
  },
  "devDependencies": {
    "@officesdk/design-components": "workspace:*",
    "@officesdk/design-icons": "workspace:*",
    "@officesdk/design-theme": "workspace:*",
    "@officesdk/design-utils": "workspace:*"
  },
  "scripts": {
    "build": "node build.js",
    "clean": "rm -rf dist"
  }
}
